{
    "name": "bristol-su/support",
    "description": "SDK for the Bristol SU Portal",
    "license": "MIT",
    "prefer-stable": false,
    "keywords": [
        "portal",
        "sdk",
        "bristol su"
    ],
    "authors": [
        {
            "name": "Toby Twigger",
            "email": "toby.twigger@bristol.ac.uk"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0",
        "laravel/passport": "^8.0",
        "orchestra/testbench": "^4.0",
        "laracasts/utilities": "^3.0",
        "ext-json": "*",
        "tobytwigger/form-schema-generator": "^1.1",
        "bristol-su/control": "@dev"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.3",
        "brianium/paratest": "^3.0"
    },
    "autoload": {
        "files": [
            "src/helpers/helpers.php"
        ],
        "psr-4": {
            "BristolSU\\Support\\": "src/"
        },
        "classmap": [
            "database/seeds/",
            "database/factories/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Support\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BristolSU\\Support\\SupportServiceProvider"
            ],
            "dont-discover": [
                "laravel/passport"
            ]
        }
    }
}
